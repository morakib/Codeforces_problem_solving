#include<stdio.h>
#include<math.h>
    int main(){
        int A[5][5];
        int i,j;
        for(i=1;i<=5;i++){
            for(j=1;j<=5;j++){
                scanf("%d",&A[i][j]);
                if(A[i][j]==1){
                    printf("%d\n",abs(i-3)+abs(j-3));
                }
            }
        }
 
    return 0;
}
